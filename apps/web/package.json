{
  "name": "@odir/web",
  "private": true,
  "scripts": {
    "postinstall": "shx cp -n src/env/local.template.ts src/env/local.ts && shx cp -n src/env/local.ts src/env/env.ts",
    "start": "npm run start:local",
    "start:local": "shx cp src/env/local.ts src/env/env.ts && next dev --port 8000",
    "test": "shx cp src/env/local.ts src/env/env.ts && TZ=utc vitest run",
    "test:watch": "shx cp src/env/local.ts src/env/env.ts && TZ=utc vitest",
    "lint": "eslint 'src' 'serverless.ts'",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:local",
    "build:local": "shx cp src/env/local.ts src/env/env.ts && npm run build:common",
    "build:dev": "shx cp src/env/dev.ts src/env/env.ts && npm run build:common",
    "build:prod": "shx cp src/env/prod.ts src/env/env.ts && npm run build:common",
    "build:common": "npm run type-check && shx rm -rf dist && next build",
    "type-check": "tsc",
    "deploy:dev": "npm run build:dev && serverless deploy --verbose",
    "deploy:prod": "npm run build:prod && serverless deploy --verbose"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.4",
    "@odir/eslint-config": "*",
    "@odir/server": "*",
    "@odir/shared": "*",
    "@reach/accordion": "^0.18.0",
    "@reach/dialog": "^0.18.0",
    "@reach/disclosure": "^0.18.0",
    "@reach/listbox": "^0.18.0",
    "@serverless/typescript": "^3.38.0",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^14.0.0",
    "@types/aos": "^3.0.4",
    "@types/canvas-confetti": "^1.4.2",
    "@types/jsonexport": "^3.0.2",
    "@types/node": "^22.13.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-timeago": "^4.1.3",
    "@vitejs/plugin-react": "^4.3.4",
    "aos": "^2.3.4",
    "autoprefixer": "^10.3.4",
    "axios": "^1.10.0",
    "axios-cache-interceptor": "^1.8.0",
    "canvas-confetti": "^1.4.0",
    "clsx": "^2.1.1",
    "eslint": "^8.44.0",
    "haversine-distance": "^1.2.1",
    "jsdom": "^26.0.0",
    "jsonexport": "^3.2.0",
    "next": "^15.1.6",
    "oidc-client": "^1.11.5",
    "p-debounce": "^2.1.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-hotkeys-hook": "^4.4.1",
    "react-markdown": "^8.0.7",
    "react-select": "^5.7.4",
    "react-timeago": "^7.1.0",
    "serverless": "^3.40.0",
    "serverless-s3-sync": "^3.4.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^3.0.4"
  }
}
